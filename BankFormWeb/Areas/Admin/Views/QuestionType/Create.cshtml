@model QuestionType

<form method="post">

    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Create QuestionType</h2>
            <hr class="row mw-80 ">
        </div>
        <div class="formgroup">
            <label asp-for="QuestionTypes"></label>
            <input asp-for="QuestionTypes" class="form-control">
            <span asp-validation-for="QuestionTypes"></span>
        </div>
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary" style="width:150px;">Create</button>
        <button asp-controller="Template" asp-action="Index" class="btn btn-secondary" >Back To List</button>
    </div>

</form>



@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
    }