@model IEnumerable<QuestionType>


<table class="table">
  <thead>
    <tr>
      <th scope="col" class="col-1 text-center">#</th>
      <th scope="col" class="col-4 text-center">QuestionType Name</th>
      <th scope="col" class="col-2 text-center">Order</th>
      <th scope="col" class=" text-center"  >options</th>
    </tr>
  </thead>
  <tbody>
        @{
            var i = 1;
        }
      @foreach(var obj in Model)
      {    
            
            <tr>
                <td class="col-1 text-center" scope="row">@(i++)</td>
            <td class="col-4 text-center"  >@obj.QuestionTypes</td>
            <td class="col-2 text-center"  >@obj.Order</td>
            <td class=" text-center"  >
                   <a  class="btn ml-3 btn-primary" asp-controller="Template" asp-action="Edit" asp-route-id="@obj.QuestionTypeId">Edit</a>
                   <a  class="btn ml-3 btn-danger" asp-controller="Template" asp-action="Delete" asp-route-id="@obj.QuestionTypeId">Delete</a>
            </td>
          </tr>
      }
  
  </tbody>
</table>
